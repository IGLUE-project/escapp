

<form method="get" action="/escapeRooms" class="escapeRoomSearch"> 
<%if (whichMenu=="public"){%>
    <input type="hidden" name="public" value="1" />
<%} else if (whichMenu=="playing"){%>
    <input type="hidden" name="own" value="1" />
<%}%>
    <div class="escapeRoomFilters">
        <input type="text" id="searchbar" name="search" placeholder="<%=i18n.common.search%>" value="<%=search%>" />
        <button type="submit" class="rounded filter-button"><span>search</span></button>
    </div>
</form> 